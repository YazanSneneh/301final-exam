<!DOCTYPE html>
<html lang="en">

<%- include('layout/head') %>

    <body>
        <%- include('layout/header') %>
            <%- include('layout/navbar') %>
                <main class="center">
                    <div class="container flex-container">
                        <% cards.forEach(card=>{ %>
                            <article class="country-card">
                                <figcaption>
                                    <h2 class=" pink-font">Date : <%= card.Date %>
                                    </h2>
                                    <h3>Cases : <%= card.Cases || 'No Cases' %>
                                    </h3>
                                </figcaption>
                            </article>
                            <% }) %>
                    </div>
                </main>
                <%- include('layout/footer') %>
    </body>

</html>